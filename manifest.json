{
	"manifest_version": 2,

	"name": "URL-to-QR Converter",
	"description": "Extension to convert the URL to QR-Code in Google Chrome.",
	"version": "0.1",

	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html",
		"default_title": "QR me!"
	},

	"permissions": [
		"tabs",
		"activeTab",
		"https://ajax.googleapis.com/"
	]
}
